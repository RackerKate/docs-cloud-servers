	<!-- Key Pair operations -->
	<method name="POST" id="createKeyPair">
		<wadl:doc xmlns="http://docbook.org/ns/docbook" xml:lang="EN" title="Create a server key pair">
			<para role="shortdesc">Creates a server key pair</para>
			<para>Creates a key pair, with the name specified in the request body, to associate with a new server
				instance.</para>
			<important>
				<title>Key Pair Naming Rules</title>
				<para> The key name must be unique and may not exceed 255 characters. It can contain the following
					characters: <itemizedlist>
						<listitem>
							<para>alphanumeric</para>
						</listitem>
						<listitem>
							<para>spaces</para>
						</listitem>
						<listitem>
							<para>dashes</para>
						</listitem>
						<listitem>
							<para>underscores</para>
						</listitem>
					</itemizedlist>
				</para>
			</important>
		</wadl:doc>
		<request>
			<representation mediaType="application/json">
				<wadl:doc xmlns="http://docbook.org/ns/docbook" xml:lang="en">
					<xsdxt:code href="../common/samples/req-json-http.txt"/>
					<xsdxt:code href="../common/samples/keypair-create-req.json"/>
