					<xsdxt:code href="../common/samples/disk-cfg-create-svr-req.xml"/>
				</wadl:doc>
			</representation>
		</request>
		<response status="202">
			<doc title="Success">Request accepted.</doc>
			<representation mediaType="application/json">
				<wadl:doc xmlns="http://docbook.org/ns/docbook" xml:lang="EN">
